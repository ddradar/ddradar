{
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous",
      "methods": ["get"],
      "route": "v1/songs"
    },
    {
      "name": "songs",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "DDRadar",
      "collectionName": "Songs",
      "sqlQuery": "SELECT c.id, c.name, c.nameKana, c.nameIndex, c.artist, c.series, c.minBPM, c.maxBPM FROM c WHERE c.nameIndex >= 0 ORDER BY c.nameIndex, c.nameKana",
      "connectionStringSetting": "COSMOS_DB_CONN_READONLY"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "scriptFile": "../dist/songs--get/index.js"
}
