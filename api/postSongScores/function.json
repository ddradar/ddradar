{
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous",
      "methods": ["post"],
      "route": "v1/scores/{songId:regex(^([01689bdiloqDIOPQ]{{32}}|\\d{{1,3}})$)}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "httpResponse"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "documents",
      "databaseName": "DDRadar",
      "collectionName": "Scores",
      "connectionStringSetting": "COSMOS_DB_CONN"
    }
  ],
  "scriptFile": "../dist/postSongScores/index.js"
}
