{
  "routes": [
    {
      "route": "/.auth/login/aad",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/google",
      "statusCode": "404"
    },
    {
      "route": "/api/v1/scores/*",
      "methods": ["POST", "DELETE"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/v1/notification",
      "methods": ["POST"],
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/api/v1/songs",
      "methods": ["POST"],
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/api/v1/user",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/v1/users/exists/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/admin/*",
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/api/v1/admin/*",
      "allowedRoles": ["administrator"]
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/200.html",
      "statusCode": 200
    }
  ]
}
