{
  "root": true,
  "plugins": ["simple-import-sort"],
  "extends": ["@nuxt/eslint-config", "plugin:nuxt/recommended", "prettier"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ],
    "import/order": "off",
    "no-console": "error",
    "simple-import-sort/imports": "error",
    "sort-imports": "off",
    "vue/no-v-model-argument": "off"
  }
}
