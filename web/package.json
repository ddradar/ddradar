{
  "name": "@ddradar/web",
  "version": "2.0.0",
  "description": "DDRadar web site (powered by Azure Static Web Apps)",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "nuxt prepare",
    "build": "nuxt build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "pretest": "nuxt prepare",
    "test": "vitest run"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@ddradar/core": "workspace:*",
    "@ddradar/db": "workspace:*",
    "@nuxt/ui-pro": "^1.0.1",
    "@oruga-ui/oruga-next": "^0.6.0",
    "@oruga-ui/theme-bulma": "^0.2.11",
    "chart.js": "^4.4.2",
    "iconv-lite": "^0.6.3",
    "markdown-it": "^14.0.0",
    "vue-chartjs": "^5.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/i18n": "^8.1.1",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^18.17.11",
    "@vue/test-utils": "^2.4.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "happy-dom": "^13.6.2",
    "nuxt": "^3.10.3",
    "nuxt-swa": "^0.4.0",
    "sass": "^1.70.0",
    "vitest": "^1.3.1"
  }
}
