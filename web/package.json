{
  "name": "@ddradar/web",
  "version": "2.0.0",
  "description": "DDRadar web site (powered by Azure Static Web Apps)",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "nuxt prepare",
    "build": "nuxt build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "pretest": "nuxt prepare",
    "test": "vitest run"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@ddradar/core": "workspace:*",
    "@ddradar/db": "workspace:*",
    "@iconify-json/flag": "^1.1.53",
    "@iconify-json/heroicons": "^1.1.21",
    "@iconify-json/simple-icons": "^1.1.107",
    "@nuxt/ui-pro": "^1.3.1",
    "chart.js": "^4.4.3",
    "iconv-lite": "^0.6.3",
    "markdown-it": "^14.1.0",
    "vue-chartjs": "^5.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/i18n": "^8.3.1",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^18.17.11",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-nuxt": "^4.0.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.12.2",
    "nuxt-swa": "^0.5.0",
    "nuxt-zod-i18n": "^1.8.0",
    "vitest": "^1.6.0"
  }
}
