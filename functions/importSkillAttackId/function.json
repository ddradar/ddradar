{
  "bindings": [
    {
      "type": "timerTrigger",
      "direction": "in",
      "name": "launchTimer",
      "schedule": "0 0 2 * * *"
    },
    {
      "name": "songs",
      "type": "cosmosDB",
      "direction": "in",
      "connection": "COSMOS_DB_CONN_READONLY",
      "databaseName": "DDRadar",
      "containerName": "Songs",
      "sqlQuery": "SELECT * FROM c WHERE c.nameIndex >= 0 AND NOT IS_DEFINED(c.skillAttackId)"
    },
    {
      "name": "$return",
      "type": "cosmosDB",
      "direction": "out",
      "connection": "COSMOS_DB_CONN",
      "databaseName": "DDRadar",
      "containerName": "Songs"
    }
  ],
  "scriptFile": "../dist/importSkillAttackId/index.js"
}
